{
    "title": "Affective Cloud Documents",
    "author": "enter",
    "description": "Affective Cloud Documents",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-sharing",
        "-lunr", 
        "-search", 
        "-fontsettings",
        "theme-fexa",
        "back-to-top-button",
        "cuav-chapters",
        "code",
        "search-pro",
        "advanced-emoji",
        "github",
        "emphasize",
        "splitter",
        "tbfed-pagefooter",
        "klipse",
        "flexible-alerts",
        "auto-scroll-table",
        "popup"
    ],
    "variables": {
        "themeFexa":{
            "nav":[
                {
                    "url":"https://www.entertech.cn",
                    "target":"_blank",
                    "name": "回车官网"
                },
                {
                    "url":"https://github.com/Entertech/Enter-Biomodule-BLE-Android-SDK",
                    "target":"_blank",
                    "name": "Android Demo"
                }
            ]
        }
    },
    "pluginsConfig": {
        "code": {
            "copyButtons": true
        },
        "github": {
            "url": "https://github.com/entertech/ACDocuments"
        },
        "flexible-alerts": {
            "note": {
                "label": "注意"
            },
            "tip": {
                "label": "小技巧"
            },
            "warning": {
                "label": "警告"
            },
            "danger": {
                "label": "危险"
            }
        },
        "theme-fexa":{
            "search-placeholder":"输入关键字搜索",
            "logo":"./logo.png",
            "favicon": "./favicon.png"
        },
        "tbfed-pagefooter": {
            "copyright":"© Entertech 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
