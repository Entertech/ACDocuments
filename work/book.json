{
    "title": "Affective Cloud Documents",
    "author": "enter",
    "description": "回车情感云计算平台",
    "language": "zh-hans",
    "stucture": {
        "readme": "回车情感云计算综述.md"
    },
    "gitbook": "3.2.3",
    "plugins": [
        "-sharing",
        "-lunr", 
        "-search", 
        "-fontsettings",
        "theme-fexa",
        "anchors",
        "image-captions",
        "back-to-top-button",
        "cuav-chapters",
        "code",
        "search-pro",
        "advanced-emoji",
        "emphasize",
        "splitter",
        "tbfed-pagefooter",
        "klipse",
        "flexible-alerts",
        "auto-scroll-table",
        "lightbox",
        "image-div"
    ],
    "variables": {
        "themeFexa":{
            "nav":[
                {
                    "url":"https://www.entertech.cn",
                    "target":"_blank",
                    "name": "回车官网"
                },
                {
                    "url":"https://github.com/Entertech/Enter-Biomodule-BLE-Android-SDK",
                    "target":"_blank",
                    "name": "Android Demo"
                }
            ]
        }
    },
    "pluginsConfig": {
        "code": {
            "copyButtons": true
        },
        "flexible-alerts": {
            "style": "flat",
            "note": {
                "label": "注意"
            },
            "tip": {
                "label": "提示"
            },
            "warning": {
                "label": "警告"
            },
            "danger": {
                "label": "危险"
            }
        },
        "theme-fexa":{
            "search-placeholder":"输入关键字搜索",
            "logo":"./logo.png",
            "favicon": "./favicon.png"
        },
        "tbfed-pagefooter": {
            "copyright":"© Entertech 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    },
    "styles": {
        "website": "styles/website.css"
    },
    "links": {
        "sidebar": {
            "蓝牙采集模块 Android SDK": "https://github.com/Entertech/Enter-Biomodule-BLE-Android-SDK",
            "蓝牙采集模块 iOS SDK": "https://github.com/Entertech/Enter-Biomodule-BLE-iOS-SDK",
            "蓝牙采集模块 SDK 和 情感云计算平台 API demo": "https://github.com/Entertech/Enter-Biomodule-Demo-Android"
        }

    }
}
